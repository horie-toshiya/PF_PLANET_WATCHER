<h4>イベント一覧</h4>
<%= link_to "新規投稿", new_admin_event_path, class: "btn btn-success" %>

 <table class="table table-hover table-inverse">
      <thead>
        <tr>
          <th>投稿ID</th>
          <th>日付</th></th>
          <th>開催場所</th>
          <th>イベント名</th>
        </tr>
      </thead>
     <tbody>
       <% @events.each do |event| %>
          <tr>
　　　　　<td><%= event.id %></td>
　　　　　<td><%= event.period %></td>
　　　　　<td><%= event.place %></td>
　　　　　<td><%= link_to event.festival,admin_event_path(event.id) %></td>
　　　　 </tr>
      <% end %>
     </tbody>
  </table>