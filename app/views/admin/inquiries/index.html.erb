<h4>お問い合わせ一覧</h4>
 <table class="table table-hover table-inverse">
      <thead>
        <tr>
          <th>投稿者</th>
          <th>日時</th>
        </tr>
      </thead>
     <tbody>
       <% @inquiries.each do |inquiry| %>
　　　　 <tr>
　　　　　<td><%= link_to inquiry.name,admin_inquiry_path(inquiry.id)%></td>
　　　　　<td><%= inquiry.created_at %></td>
　　　　 </tr>
      <% end %>
     </tbody>
  </table>