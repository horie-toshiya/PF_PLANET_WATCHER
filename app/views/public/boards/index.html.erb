<h4>投稿一覧</h4>
 <%= link_to "新規投稿", new_board_path, class: "btn btn-success" %>

<table class="table table-hover table-inverse">
      <thead>
        <tr>
          <th>投稿ID</th>
          <th>投稿時間</th>
          <th>タイトル</th>
          <th>投稿者</th>
        </tr>
      </thead>
     <tbody>
       <% @boards.each do |board| %>
         <tr>
　　　　　<td><%= board.id %></td>
　　　　　<td><%= board.created_at %></td>
　　　　　<td><%= link_to board.title,board_path(board.id) %></td>
　　　　　<td><%= board.name %></td>
　　　　 </tr>
       <% end %>
     </tbody>
  </table>